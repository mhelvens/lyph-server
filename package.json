{
  "name": "lyph-server",
  "version": "0.1.0",
  "description": "the lyph server",
  "main": "dist/index.js",
  "scripts": {
    "build-server": "webpack && node dist/tools/buildSwaggerJSON.js",
    "build-docs": "cd src && copyfiles docs/**/* ../dist",
    "build": "npm run build-server && npm run build-docs",
    "prestart": "npm run build",
    "start": "forever start -a -l forever.log -o out.log -e err.log ./dist/server/server.js",
    "stop": "forever stop dist/server/server.js",
    "prerestart": "npm run build",
    "restart": "forever restart -a ./dist/server/server.js",
    "test": "mocha --timeout 10000  --compilers js:babel-core/register"
  },
  "keywords": [
    "lyph"
  ],
  "authors": [
    "Michiel Helvensteijn <mhelvens@gmail.com>",
    "Samuel Alexander <rmhiale@ucl.ac.uk>",
    "Natallia Kokash <natallia.kokash@gmail.com>"
  ],
  "dependencies": {
    "commander": "~2",
    "cors": "~2",
    "es6-promisify": "~3",
    "express": "~4",
    "forever": "~0.15",
    "lodash": "~4",
    "node-rest-client": "~1",
    "promisify": "~0.0",
    "swagger-express-middleware": "~0.4",
    "open-physiology-model": "open-physiology/open-physiology-model",
    "utilities": "open-physiology/utilities",
    "rxjs": "~5"
  },
  "devDependencies": {
    "babel": "6",
    "babel-cli": "^6.10.1",
    "babel-core": "6",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-instanceof": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "6",
    "babel-preset-es2015": "6",
    "chai": "~3",
    "copyfiles": "^1.0.0",
    "csv": "~0.4",
    "csv-parse": "~1",
    "highland": "~2",
    "json-loader": "~0.5",
    "mocha": "~2",
    "mocha-webpack": "^0.5.0",
    "power-assert": "^1.4.1",
    "progress": "~1",
    "raw-loader": "~0.5",
    "superagent": "latest",
    "superagent-promise": "~1",
    "supertest": "~1",
    "supertest-as-promised": "~2",
    "swagger-ui": "~2",
    "source-map-support": "^0.4.2",
    "thenify": "~3",
    "thenify-all": "~1",
    "webpack": "^1.13.1",
    "webpack-node-externals": "1.5"
  }
}
